{
  "private": true,
  "name": "minimal-blog",
  "description": "Typography driven, feature-rich blogging theme with minimal aesthetics. Includes tags/categories support and extensive features for code blocks such as live preview, line numbers, and line highlighting.",
  "version": "1.2.0",
  "author": "LekoArts <hello@lekoarts.de>",
  "license": "MIT",
  "starter-name": "gatsby-starter-minimal-blog",
  "scripts": {
    "build": "gatsby clean && gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "develop:cypress": "cross-env CYPRESS_SUPPORT=y yarn develop",
    "build:cypress": "cross-env CYPRESS_SUPPORT=y yarn build",
    "start": "rm -rf .cache && gatsby develop",
    "serve": "gatsby serve --prefix-paths",
    "clean": "gatsby clean",
    "postbuild": "mv public build && mkdir public && mv build public/blog"
  },
  "dependencies": {
    "@lekoarts/gatsby-theme-minimal-blog": "^2.4.0",
    "@weknow/gatsby-remark-twitter": "^0.2.3",
    "gatsby": "^2.20.36",
    "gatsby-plugin-canonical-urls": "^2.8.0",
    "gatsby-plugin-manifest": "^2.2.3",
    "gatsby-plugin-offline": "^2.2.4",
    "gatsby-plugin-sitemap": "^2.2.19",
    "gatsby-plugin-vercel": "^1.0.3",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.10",
    "gatsby-remark-autolink-headers": "^2.3.11",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-transformer-remark": "^2.8.28",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typescript": "3.9.7"
  },
  "devDependencies": {
    "cross-env": "^5.2.0"
  },
  "prettier": {
    "printWidth": 95,
    "tabWidth": 4,
    "singleQuote": true,
    "semi": false
  }
}
